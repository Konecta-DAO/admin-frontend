[
    {
        "match": ".well-known",
        "ignore": false
    },
    {
        "match": ".well-known/ii-alternative-origins",
        "headers": {
            "Access-Control-Allow-Origin": "*",
            "Content-Type": "application/json"
        },
        "ignore": false
    },
  {
    "match": "**/*.webp",
    "headers": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "security_policy": "standard"
  },
  {
    "match": "**/*.svg",
    "headers": {
      "Cache-Control": "public, max-age=31536000, immutable"
      // Note: If you serve user-uploaded SVGs or SVGs that might contain scripts,
      // you might need a specific, stricter CSP for SVG files.
    },
    "security_policy": "standard"
  },
  {
    "match": "**/*.ico",
    "headers": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "security_policy": "standard"
  },
]